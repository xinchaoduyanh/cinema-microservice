# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:9080
NEXT_PUBLIC_WS_URL=ws://localhost:9080

# Authentication
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id_here

# Feature Flags
NEXT_PUBLIC_ENABLE_NOTIFICATIONS=true
NEXT_PUBLIC_ENABLE_DARK_MODE=true
NEXT_PUBLIC_ENABLE_GOOGLE_LOGIN=true

# App Configuration
NEXT_PUBLIC_APP_NAME=Cinema Booking
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Pagination
NEXT_PUBLIC_ITEMS_PER_PAGE=12

# Image Upload
NEXT_PUBLIC_MAX_IMAGE_SIZE=5242880
NEXT_PUBLIC_ALLOWED_IMAGE_TYPES=image/jpeg,image/png,image/webp
