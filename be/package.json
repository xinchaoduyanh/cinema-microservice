{
  "name": "nest-turbo-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:clean": "turbo run build:clean",
    "test": "turbo run test",
    "dev": "dotenv -e .env -- turbo run start:dev --parallel",
    "prod": "turbo run start:prod --parallel",
    "migration:up": "turbo run migration:up --parallel",
    "lint": "turbo run lint",
    "prepare": "husky",
    "format": "prettier --write \"apps/**/src/**/*.ts\" \"libs/**/src/**/*.ts\""
  },
  "engines": {
    "npm": ">=10.0.0",
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.17.0",
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@mikro-orm/cli": "^6.5.8",
    "@nestjs/cli": "^11.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/express": "^5.0.0",
    "@types/google-libphonenumber": "^7.4.30",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "turbo": "latest",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "dotenv-cli": "^10.0.0",
    "dotenv": "^17.2.3",
    "husky": "^9.1.7"
  }
}
